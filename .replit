modules = ["web", "python-3.11", "nodejs-20", "nix", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["fontconfig", "ghostscript", "glib", "harfbuzz", "openssl", "pango", "postgresql"]

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "main:app"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn --bind 0.0.0.0:5000 --reuse-port --reload --workers 4 --timeout 120 main:app"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true
integrations = ["stripe:2.0.0", "python_openai_ai_integrations:1.0.0"]

